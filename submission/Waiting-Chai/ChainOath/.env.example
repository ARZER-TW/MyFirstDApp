# 网络配置
# 可选值: mainnet, sepolia, localhost
VITE_NETWORK=sepolia

# Sepolia 测试网配置
VITE_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
VITE_SEPOLIA_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_MAINNET_NFT_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# 主网配置（生产环境使用）
VITE_MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
VITE_MAINNET_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_MAINNET_NFT_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# 本地开发网络配置
VITE_LOCALHOST_RPC_URL=http://127.0.0.1:8545
VITE_LOCALHOST_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_LOCALHOST_NFT_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000